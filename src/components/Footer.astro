<script type="text/javascript">
	document.getElementById('aboutMeLink').onclick = function() {
	  this.classList.toggle('closeFlyout');
	  const flyoutElement = document.getElementById('flyout');
	  flyoutElement.classList.toggle('flyoutExpanded');
	  if (!flyoutElement.classList.contains('flyoutTransition')) {
		flyoutElement.classList.add('flyoutTransition');
	  }
	};
</script>